    $("#dialog-error").dialog({
        resizable: false,
        modal: true,
        title: "Save Info",
        height: "auto",
        width: 413,
        buttons: {
            "OK":{click :function () {
                $(this).dialog('close');
            },
                text: 'OK',
                'class': 'vabutton1'
            }
        },
        open: function( event, ui ) {
            $('.ui-dialog :button').blur();
        }
    });
    $('.ui-dialog :button').blur();
}
